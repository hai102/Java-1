package com.mycompany.demobuoi5;

/**
 *
 * @author admin
 */
public class TBCChia3 {
    public static void main(String[] args) {
        double tong = 0;
        int i = 27;
        double dem = 0;
        while(i < 251){
            if(i % 3 == 0){
                tong += i;
                dem++;
            }
            i++;
        }
        System.out.println("Ket qua la: "+ tong / dem);
    }
}
